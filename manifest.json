{
    "name": "Uribbble",
    "description": "Quick feedback component for dribbble",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://dribbble.com/*"],
            "css": ["style.css"],
            "js": ["content.js"]
        }
    ],
    "permissions": ["tabs"]
}
